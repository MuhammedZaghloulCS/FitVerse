
@{
    Layout = "~/Views/Shared/_CoachLayout.cshtml";
    ViewBag.Active = "DietPlans";
    ViewBag.PageTitle = "Diet Plans";
}
@model FitVerse.Core.ViewModels.DietPlan.DietPlanDashboardVM

<!-- Main Content -->
<div class="container-fluid px-4 py-4">
    <!-- Enhanced Page Header -->
    <div class="d-flex justify-content-between align-items-center mb-4">
        <div>
            <h2 class="fw-bold mb-1">
                <i class="bi bi-egg-fried me-2 text-primary"></i>Diet Plans
            </h2>
            <p class="text-muted mb-0">Create and manage comprehensive nutrition plans for your clients</p>
        </div>
        <div class="d-flex gap-2">
            <button class="btn btn-outline-primary modern-btn" id="refreshPlansBtn">
                <i class="bi bi-arrow-clockwise me-2"></i>Refresh
            </button>
            <button class="btn btn-primary-custom" data-bs-toggle="modal" data-bs-target="#createDietPlanModal">
                <i class="bi bi-plus-circle me-2"></i>Create Diet Plan
            </button>
        </div>
    </div>

    <!-- Enhanced Stats Cards -->
    <div class="row g-4 mb-4">
        <div class="col-lg-3 col-md-6">
            <div class="stat-card primary w-100">
                <div class="stat-icon primary"><i class="bi bi-egg-fried"></i></div>
                <div class="stat-content">
                    <div class="stat-value">@Model.DietPlanCount</div>
                    <div class="stat-label">Total Diet Plans</div>
                </div>
                <div class="stat-trend positive">
                    <i class="bi bi-arrow-up"></i>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="stat-card success w-100">
                <div class="stat-icon success"><i class="bi bi-people"></i></div>
                <div class="stat-content">
                    <div class="stat-value">@Model.ClientFollowingCount</div>
                    <div class="stat-label">Clients Following</div>
                </div>
                <div class="stat-trend positive">
                    <i class="bi bi-arrow-up"></i>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="stat-card warning w-100">
                <div class="stat-icon warning"><i class="bi bi-calendar-check"></i></div>
                <div class="stat-content">
                    <div class="stat-value" id="activePlansCount">-</div>
                    <div class="stat-label">Active Plans</div>
                </div>
                <div class="stat-trend positive">
                    <i class="bi bi-arrow-up"></i>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-6">
            <div class="stat-card info w-100">
                <div class="stat-icon info"><i class="bi bi-graph-up"></i></div>
                <div class="stat-content">
                    <div class="stat-value" id="avgCaloriesCount">-</div>
                    <div class="stat-label">Avg Calories</div>
                </div>
                <div class="stat-trend positive">
                    <i class="bi bi-arrow-up"></i>
                </div>
            </div>
        </div>
    </div>





    <!-- Enhanced Search and Filters -->
    <div class="card-custom mb-4">
        <div class="card-body-custom">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="search-wrapper">
                        <i class="bi bi-search search-icon"></i>
                        <input type="text" id="searchDietPlan" class="form-control-custom search-input" placeholder="Search diet plans by name, goal, or calories...">
                    </div>
                </div>
                <div class="col-lg-6">
                    <div class="d-flex gap-2 justify-content-lg-end">
                        <div class="dropdown">
                            <button class="btn btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown">
                                <i class="bi bi-funnel me-2"></i>Filter by Goal
                            </button>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item filter-option" data-filter="all">All Plans</a></li>
                                <li><a class="dropdown-item filter-option" data-filter="weight-loss">Weight Loss</a></li>
                                <li><a class="dropdown-item filter-option" data-filter="muscle-gain">Muscle Gain</a></li>
                                <li><a class="dropdown-item filter-option" data-filter="maintenance">Maintenance</a></li>
                            </ul>
                        </div>
                        <div class="dropdown">
                            <button class="btn btn-outline-secondary dropdown-toggle" type="button" data-bs-toggle="dropdown">
                                <i class="bi bi-sort-down me-2"></i>Sort by
                            </button>
                            <ul class="dropdown-menu">
                                <li><a class="dropdown-item sort-option" data-sort="name">Name</a></li>
                                <li><a class="dropdown-item sort-option" data-sort="calories">Calories</a></li>
                                <li><a class="dropdown-item sort-option" data-sort="created">Date Created</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- Enhanced Diet Plans Grid -->
    <div class="card-custom">
        <div class="card-header-custom">
            <div class="d-flex justify-content-between align-items-center">
                <h5 class="mb-0">
                    <i class="bi bi-grid me-2"></i>Your Diet Plans
                </h5>
                <div class="view-toggle">
                    <div class="btn-group" role="group">
                        <button type="button" class="btn btn-outline-secondary btn-sm active" id="gridViewBtn">
                            <i class="bi bi-grid-3x3-gap"></i>
                        </button>
                        <button type="button" class="btn btn-outline-secondary btn-sm" id="listViewBtn">
                            <i class="bi bi-list"></i>
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div class="card-body-custom">
            <!-- Loading State -->
            <div class="loading-state text-center py-5" id="loadingState" style="display: none;">
                <div class="spinner-border text-primary mb-3" role="status">
                    <span class="visually-hidden">Loading...</span>
                </div>
                <p class="text-muted">Loading your diet plans...</p>
            </div>
            
            <!-- Plans Container -->
            <div class="row g-4" id="dietPlansContainer">
                <!-- Plans will be loaded here dynamically -->
            </div>
            
            <!-- Empty State -->
            <div class="empty-state text-center py-5" id="emptyState" style="display: none;">
                <i class="bi bi-egg-fried fs-1 text-muted mb-3"></i>
                <h5 class="text-muted mb-2">No Diet Plans Found</h5>
                <p class="text-muted mb-4">Create your first diet plan to get started</p>
                <button class="btn btn-primary-custom" data-bs-toggle="modal" data-bs-target="#createDietPlanModal">
                    <i class="bi bi-plus-circle me-2"></i>Create Your First Plan
                </button>
            </div>
        </div>
    </div>
